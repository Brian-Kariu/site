---
import { Icon } from 'astro-icon/components'
import data from '@/data/data.json'

const githubLink = data.links.github
const linkedinLink = data.links.linkedin
---

<a href={githubLink} target='_blank' rel='noopener' aria-label='github'
	><Icon
		class='h-8 dark:text-white hover:text-orange dark:hover:text-orange md:h-12'
		name='mdi:github'
		size={100}
	/></a
>
<a href={linkedinLink} target='_blank' rel='noopener' aria-label='linkedin'
	><Icon
		class='h-8 dark:text-white hover:text-orange dark:hover:text-orange md:h-12'
		name='mdi:linkedin'
		size={100}
	/></a
>
